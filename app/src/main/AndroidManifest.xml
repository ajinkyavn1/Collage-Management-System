<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.gpj.govermentpolytechnicjalgaon">
    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name=".Student.Student_Main"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Teacher.Teacher_Main"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Teacher.Teacher_Register" />
        <activity android:name=".Teacher.Teachers" />
        <activity android:name=".Student.Student" />
        <activity android:name=".Student.activity_register" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>